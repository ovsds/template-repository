{
  "private": true,
  "name": "template-repository",
  "devDependencies": {
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "^19.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1"
  },
  "hooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint"
  },
  "lint-staged": {
